$phone-down = 'screen and (max-width: 767px)'
$tablet-up = 'screen and (min-width: 768px)'
$desktop-up = 'screen and (min-width: 992px)'
$desktop-large-up = 'screen and (min-width: 1200px)'

$color-navy = #1b1b28
$color-white = #ffffff
$color-red = #e11133

$font-primary = 'Montserrat', sans-serif;

$weight-regular = 400;
$weight-semibold = 600;
$weight-bold = 700;
$weight-extrabold = 800;

$color-darkest-navy = #1b1b28
$color-darker-navy = #282837
$color-dark-navy = #343440
$color-mid-navy = #4e4e58
$color-lighter-navy = #9a9a9f
$color-lightest-navy = #cccccf



.visible-xs
    @media $tablet-up
        display none

.hidden-xs
    @media $phone-down
        display none !important
.hidden-sm
    @media $tablet-up
        display none !important

.hidden-lg
    @media $desktop-up
        display none !important

get(hash, key)
    return pair[1] if pair[0] == key for pair in hash


